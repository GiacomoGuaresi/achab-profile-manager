{
  "name": "achab-profile-manager",
  "version": "1.0.0",
  "main": "public/electron.js",
  "homepage": "./",
  "scripts": {
    "start": "electron .",
    "react-build": "webpack --config webpack.config.js",
    "dev": "npm run react-build && electron .",
    "build": "react-scripts build",
    "electron:build": "electron-builder"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "Profile Manager for Orcaslicer",
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "babel-loader": "^10.0.0",
    "css-loader": "^7.1.2",
    "electron": "^36.4.0",
    "electron-builder": "^26.0.12",
    "html-webpack-plugin": "^5.6.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "style-loader": "^4.0.0",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.1.1",
    "@mui/material": "^7.1.1",
    "elkjs": "^0.10.0",
    "follow-redirects": "^1.15.9",
    "icons-material": "^0.0.1-security",
    "path-browserify": "^1.0.1",
    "react-force-graph": "^1.47.6",
    "react-force-graph-2d": "^1.27.1",
    "react-router-dom": "^7.6.2",
    "react-scripts": "^5.0.1",
    "reactflow": "^11.11.4"
  },
  "build": {
    "productName": "Achab Profile Manager",
    "appId": "com.achab.profilemanager",
    "directories": {
      "buildResources": "assets",
      "output": "dist"
    },
    "files": [
      "build/**/*",
      "build/electron.js"
    ],
    "win": {
      "target": [
        "nsis",
        "portable"
      ],
      "icon": "assets/icon.ico"
    },
    "mac": {
      "target": "dmg",
      "icon": "assets/icon.icns"
    },
    "linux": {
      "target": "AppImage",
      "icon": "assets/icon.png"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "GiacomoGuaresi",
        "repo": "achab-profile-manager"
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
